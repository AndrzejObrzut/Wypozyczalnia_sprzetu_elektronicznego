<h1>Photos#index</h1>
<p>Find me in app/views/photos/index.html.erb</p>

<p>
  <%= @photos.size %>
</p>


<div class="container">
  <div class="grid">
    <div class="row">
      <h1>PHOTOS</h1>
    </div>
      <%  @photos.each do |photo|  %>
        <%= image_tag(url_for(:controller => "photos", :action => "show", :id => photo.id), size: "200x200", alt: photo.name) %>
        <br>
      <% end %>
  </div>

</div>
